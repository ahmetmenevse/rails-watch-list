<p style="color: green"><%= notice %></p>

<h1>lists</h1>

<div class="row">
    <% @lists.each do |list| %>
      <div class="co-12 col-sm-6 col-md-4 mb-3">
        <%= link_to list_path(list), class:"text-decoration-none" do %>
          <%= list.name %>
        <% end %>
      </div>
    <% end %>
</div>

<%= link_to "New list", new_list_path %>
